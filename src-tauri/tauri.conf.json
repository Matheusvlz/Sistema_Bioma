{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "sistema_bioma",
  "version": "0.1.0",
  "identifier": "com.sistemabioma.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "maximized": true,
        "height": 600,
        "resizable": true,
        "label": "main",
        "title": "Sistema Bioma",
        "width": 800,
        "minWidth": 400,
        "minHeight": 300
      },
      {
        "label": "cadastrar-consultor",
        "url": "/",
        "title": "Cadastrar Consultor",
        "width": 1200,
        "height": 800,
        "visible": false,
        "center": true,
        "resizable": true,
        "minWidth": 900,
        "minHeight": 650
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "config.json"
    ]
  },
  "plugins": {
  "dialog": null,
  "shell": null
}

}