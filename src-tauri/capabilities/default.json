{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window and cadastro-clientes",
  "windows": ["*"],
  "permissions": [
    "core:default",
    "opener:default",
    "core:webview:allow-create-webview-window",
    "core:window:allow-set-focus",
    "core:window:allow-close",
    "dialog:default",
    "dialog:allow-save",
    "dialog:allow-open",
    "dialog:allow-message",
    "dialog:allow-confirm",
    "dialog:allow-ask",
    "fs:default",
    "fs:allow-write-file",
    "fs:allow-read-file",
    "fs:allow-create",
    "fs:allow-remove",
    "fs:allow-rename",
    "fs:allow-copy-file",
    "fs:allow-mkdir",
    "fs:allow-read-dir",
    "fs:allow-exists"
  ]
}
